{
    "manifest_version": 2,
    "name": "Send to Touch Dev Weekly",
    "description": "Send your recommendations to Touch Dev Weekly",
    "version": "1.1.2",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "options_page": "options.html",
    "permissions": [
        "tabs", "storage", "notifications", "contextMenus"
    ],
    "background": {
        "scripts": ["background.js", "context.js", "jquery-3.1.0.min.js"]
    }
 }
