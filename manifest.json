{
  "manifest_version": 2,
  "name": "Send to Touch Dev Weekly",
  "description": "Extension to send links recommendations to Touch Dev Weekly",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
 }